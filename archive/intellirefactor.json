{
  "refactoring_settings": {
    "analysis_results_dir": "analysis_results3"
  },
  "analysis": {
    "max_file_size": 2097152,
    "excluded_patterns": [
      "*.pyc",
      "__pycache__",
      ".git",
      ".venv",
      "venv",
      "node_modules",
      "*.egg-info",
      "build",
      "dist",
      "htmlcov",
      ".pytest_cache",
      ".ruff_cache",
      ".hypothesis"
    ],
    "included_patterns": [
      "*.py"
    ],
    "metrics_thresholds": {
      "cyclomatic_complexity": 15.0,
      "maintainability_index": 15.0,
      "lines_of_code": 1000
    },
    "analysis_depth": 15
  },
  "refactoring": {
    "safety_level": "moderate",
    "auto_apply": false,
    "backup_enabled": true,
    "validation_required": true,
    "max_operations_per_session": 100
  },
  "knowledge": {
    "knowledge_base_path": "knowledge",
    "auto_learn": true,
    "confidence_threshold": 0.6,
    "max_knowledge_items": 20000
  },
  "plugins": {
    "plugin_directories": ["plugins"],
    "auto_discover": true,
    "enabled_plugins": []
  }
}